"#_L_back": {
	text: "    Principal    "
},
"#_L_actual": {
	text: "Eventos"
},
"#_L_alterno": {
	text: "    Categorías    "
},
"#_V_eventos": {
	height: "auto",
	width: "100%",
	top: "50dp",
	left: 0
},
"#_Tbl_eventos": {
	height: "auto",
	width: "100%",
	top: 0,
	left: 0,
	backgroundColor: "transparent",
    top: 0
},
"#_Tbl_eventos[platform=ios]": {
	filterAttribute: "myFilter" //Se busca por lo que contiene el Label eventos en cada fila, sólo si es iOS, si 
								//es android la búsqueda queda por defecto, ver en el child.js para entender por qué
},
