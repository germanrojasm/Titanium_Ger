	height: "auto",
	width: "100%",
	top: 0,
	left: 0,
	backgroundColor: "transparent",
    top: 0
},
"#_Tbl_categorias[platform=ios]": {
	filterAttribute: "myFilter" //Se busca por lo que contiene el Label eventos en cada fila, sólo si es iOS, si 
