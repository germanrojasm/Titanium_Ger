	height: "auto",
	width: "100%",
	left: 0,
	color: "#000000",
	font: {
        fontSize: '15dp'
    },
    textAlign: Titanium.UI.TEXT_ALIGNMENT_CENTER,
    text: "eventos"
},

"#_L_categorias":{
	height: "auto",
	width: "100%",
	left: 0,
	color: "#000000",
	font: {
        fontSize: '15dp'
    },
